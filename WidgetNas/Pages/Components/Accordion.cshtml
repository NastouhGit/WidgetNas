@page
@model WidgetNas.Pages.Components.AccordionModel
@{
    ViewData["Title"] = "آکاردئون Accordion";
    ViewData["Description"] = "ویجت نس یک فریم ورک فارسی با پشتیبانی کامل از راست به چپ و همچنین چپ به راست است.";
